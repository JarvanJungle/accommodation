<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://services.sabre.com/hotel/avail/v4_0_0" targetNamespace="http://services.sabre.com/hotel/avail/v4_0_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:complexType name="GeoRef">
        <xs:annotation>
            <xs:documentation>
                GeoRef tag is used to search Hotel List information on basis of latitute longitude address and ref point.
            </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="GeoCode">
                <xs:annotation>
                    <xs:documentation>
                        Search by Latitude and Longitude.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attributeGroup ref="LatLonPair" />
                </xs:complexType>
            </xs:element>
            <xs:element name="RefPoint">
                <xs:annotation>
                    <xs:documentation>
                        Search by Reference points such as Airport Code
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Value" type="StringLength3to50" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                Geo Reference point value.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ValueContext" type="Type" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                Geo Reference point context.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="RefPointType" type="RefPointType" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                OTA Codes for  RefRefPointType.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="StateProv" type="StringLength2to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                State or Province of the Address Search.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CountryCode" type="StringLength2to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                Geo Reference points Country.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="AddressRef">
                <xs:annotation>
                    <xs:documentation>Search by Address.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Street" type="StringLength0to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>Street name of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="City" type="StringLength0to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>City name of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="County" type="StringLength0to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>Country  name of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PostalCode" type="StringLength0to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>Postal code of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="StateProv" type="StringLength0to50" use="optional">
                        <xs:annotation>
                            <xs:documentation>State or Province of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CountryCode" type="StringLength0to50" use="required">
                        <xs:annotation>
                            <xs:documentation>Country Code of the Address Search.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:choice>
        <xs:attribute name="Radius" type="xs:double" use="required">
            <xs:annotation>
                <xs:documentation>Radius to be included in the search from the search point.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UOM" type="UnitOfMeasureType" use="required">
            <xs:annotation>
                <xs:documentation>
                    Used for specifing what unit will be used for search radius distance.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:attributeGroup name="LatLonPair">
        <xs:annotation>
            <xs:documentation>Latitude or longitude pair</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Latitude" use="required">
            <xs:annotation>
                <xs:documentation>Latitude of the Geo Search</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:double">
                    <xs:minInclusive value="-90" />
                    <xs:maxInclusive value="90" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Longitude" use="required">
            <xs:annotation>
                <xs:documentation>Longitude of the Geo Search</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:double">
                    <xs:minInclusive value="-180" />
                    <xs:maxInclusive value="180" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:simpleType name="Type">
        <xs:annotation>
            <xs:documentation>
                Used for specifing what line of business will be returned.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="NAME" />
            <xs:enumeration value="CODE" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RefPointType">
        <xs:annotation>
            <xs:documentation>
                OTA Codes for  RefRefPointType.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="5" />
            <xs:enumeration value="6" />
            <xs:enumeration value="7" />
            <xs:enumeration value="11" />
            <xs:enumeration value="16" />
            <xs:enumeration value="18" />
            <xs:enumeration value="37" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="UnitOfMeasureType">
        <xs:annotation>
            <xs:documentation>
                Used for specifing what unit will be used for search radius distance.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="MI" />
            <xs:enumeration value="KM" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="HotelPref">
        <xs:annotation>
            <xs:documentation>
                HotelPref describes filtering criterias.  It is optional
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="1">
            <xs:element name="BrandCodes" type="BrandCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Filter by BrandCodes
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChainCodes" type="ChainCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Filter by ChainCodes
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AmenityCodes" type="AmenityCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        AmenityCodes -   Optional. AmenityCode Limit - Maximum 20.
                        Inclusive  -  Boolean - True or False. Defaults to False.  True means returned properties must have all the requested Amenity Codes. False means returned properties must have atleast one of the requested codes
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecurityFeatureCodes" type="SecurityFeatureCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Filter by SecurityFeatureCodes
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyTypeCodes" type="PropertyTypeCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        PropertyTypeCodes OTA Codes  PCT   Optional. Limit - Maximum 20.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyQualityCodes" type="PropertyQualityCodes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        PropertyQualityCodes OTA Codes  LOS   Optional. Limit - Maximum 20.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SabreRating" type="SabreRating" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Optional. Sabre rating currently uses NTM rating in GDS. MinRating or MaxRating  between 1 to 5. i.e 1,2,3,4,5.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="HotelName" type="StringLength3to100" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Optional. if given then atleast 3 characters should be specified. Return Hotels matching the search term
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="BrandCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit  Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="BrandCode" type="AlphaNumericLength1to20">
                <xs:annotation>
                    <xs:documentation xml:lang="en"> Hotel Brand Code. </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChainCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit  Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="ChainCode" type="AlphaNumericLength2">
                <xs:annotation>
                    <xs:documentation xml:lang="en"> Hotel Chain Code. </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AmenityCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit  Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="AmenityCode" type="OTACodeType">
                <xs:annotation>
                    <xs:documentation>
                        Amenity Code
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Inclusive" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>AmenityCodes Inclusive    Boolean  True or False. Defaults to False.  True means returned propertis must have all the requested Amenity Codes.
                    False means returned properties must have atleast one of the requested codes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="SecurityFeatureCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit  Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="SecurityFeatureCode" type="OTACodeType">
                <xs:annotation>
                    <xs:documentation>
                        Security Feature Code
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Inclusive" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>SecurityFeatureCodes Inclusive  Boolean  True or False. Defaults to False.  True means returned propertis must have all the requested Security Feature Codes.
                    False means returned properties must have atleast one of the requested codes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="PropertyTypeCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit - Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="PropertyTypeCode" type="OTACodeType">
                <xs:annotation>
                    <xs:documentation>
                        PropertyTypeCode OTA Code  PCT.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Inclusive" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>PropertyTypeCodes Inclusive    Boolean  True or False. Defaults to False.  True means returned propertis must have all the requested PropertyType Codes.
                    False means returned properties must have atleast one of the requested codes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="PropertyQualityCodes">
        <xs:annotation>
            <xs:documentation>
                Optional. Limit - Maximum 20
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="20">
            <xs:element name="PropertyQualityCode" type="OTACodeType">
                <xs:annotation>
                    <xs:documentation>
                        PropertyQualityCode OTA Code  LOS.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Inclusive" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>PropertyQualityCodes Inclusive    Boolean  True or False. Defaults to False.  True means returned propertis must have all the requested PropertyQualityCodes.
                    False means returned properties must have atleast one of the requested codes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="SortByType">
        <xs:annotation>
            <xs:documentation>
                Used for specifing SortType
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="DistanceFrom" />
            <xs:enumeration value="TotalRate" />
            <xs:enumeration value="SabreRating" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="SabreRating">
        <xs:annotation>
            <xs:documentation>
                Optional. Sabre rating currently uses NTM rating in GDS. MinRating or MaxRating  between 0.5 to 5. i.e 1,2,3,4,5.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="Min" type="NumericRange0to5WithDecimalForRating">
            <xs:annotation>
                <xs:documentation>
                    Sabre rating currently uses NTM rating in GDS. MinimumRating   Range 0,0.5,1,1.0,1.5,...4.5,5
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Max" type="NumericRange0to5WithDecimalForRating">
            <xs:annotation>
                <xs:documentation>
                    Sabre rating currently uses NTM rating in GDS. MaximumRating   Range 0,0.5,1,1.0,1.5,...4.5,5
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ImageRef">
        <xs:annotation>
            <xs:documentation>Hotel Image Details</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Type" type="ImageTypes" use="optional" default="THUMBNAIL">
            <xs:annotation>
                <xs:documentation>Image types</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CategoryCode" type="xs:positiveInteger" use="optional">
            <xs:annotation>
                <xs:documentation>Category of Image being requested</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="LanguageCode" type="LanguageCode" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Two character Language Code.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="ImageTypes">
        <xs:annotation>
            <xs:documentation>Image types</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="ORIGINAL" />
            <xs:enumeration value="THUMBNAIL" />
            <xs:enumeration value="SMALL" />
            <xs:enumeration value="MEDIUM" />
            <xs:enumeration value="LARGE" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LanguageCode">
        <xs:annotation>
            <xs:documentation xml:lang="en">Two character Language Code.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="([a-z][a-z][_][A-Z][A-Z])|([a-zA-Z]{2})" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="HotelInfo">
        <xs:annotation>
            <xs:documentation xml:lang="en">Details for the Hotel Property.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="TierLabels" type="TierLabels" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">TierLabels is use show Hotel property to any agent as thier  requirement.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LocationInfo" type="LocationInfo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Location and Address details for the Hotel Property.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amenities" type="AmenityInfos" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Ameneties for a hotel</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecurityFeatures" type="SecurityFeatureInfos" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>List of SecurityFeatures provided by the property</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyTypeInfo" type="PropertyTypeInfos" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Property Class type information</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyQualityInfo" type="PropertyQualityInfos" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Property Class quality information</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="HotelCode" type="StringLength1to20" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Unique code which identifies the Hotel Property</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CodeContext" type="CodeContextType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en"> ENUM type. Values -SABRE or GLOBAL in capital. It identifies whether the given HotelCode is Sabre property ID or Global property ID</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="HotelName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Hotel Name. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ChainCode" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Hotel Chain Code. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ChainName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Hotel Chain Name. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BrandCode" type="StringLength0to50" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Hotel BrandCode. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BrandName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Hotel BrandName. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Distance" type="xs:double" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en"> Distance of Hotel from the search point. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Direction" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">  Direction of Hotel from the search point. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UOM" type="UnitOfMeasureType" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Used for specifing what unit will be used for search radius distance.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Logo" type="xs:anyURI" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Logo Image of the Hotel
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SabreRating" type="StringLength0to10" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Optional. Sabre rating currently uses NTM rating in GDS. Min or Max  between 1 to 5. i.e 1,2,3,4,5.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Ordinal" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Displays position at which preferred properties are shown in the response</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SabreHotelCode" type="StringLength1to20">
            <xs:annotation>
                <xs:documentation xml:lang="en">Holds SabreID</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="SecurityFeatureInfos">
        <xs:annotation>
            <xs:documentation>List of SecurityFeatureInfos provided by the property</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="SecurityFeature" type="SecurityFeature">
                <xs:annotation>
                    <xs:documentation>Security Feature Information</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SecurityFeature">
        <xs:annotation>
            <xs:documentation>Security Feature Information</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="Code" type="OTACodeType">
                    <xs:annotation>
                        <xs:documentation>Numeric OTA code for the Security feature</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Description" type="StringLength0to255">
                    <xs:annotation>
                        <xs:documentation>Security Description</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="TierLabels">
        <xs:annotation>
            <xs:documentation>TierLabels is use show Hotel property to any agent as thier  requirement.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TierLabel" minOccurs="0" maxOccurs="5">
                <xs:annotation>
                    <xs:documentation>
                        TierLabels is use show Hotel property to any agent as thier  requirement.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="StringLength0to500" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                Optional. Type of tier label .
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Value" type="StringLength0to500" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                Optional. value of tier label .
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ProgramId" type="NumericLength10" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                Optional. ProgramId of tier label .
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="LocationInfo">
        <xs:annotation>
            <xs:documentation xml:lang="en">Location and Address details for the Hotel Property.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="Address" type="Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Address details for the Hotel Property.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Neighborhoods" type="Neighborhoods" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Neighborhoods</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Contact" type="Contact" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contact details for the Hotel Property.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Latitude" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation>Latitude of the  the Hotel Property</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Longitude" type="StringLength0to20" use="optional">
