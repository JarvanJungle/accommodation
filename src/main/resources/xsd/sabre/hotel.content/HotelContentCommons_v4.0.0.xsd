<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://services.sabre.com/hotel/content/v4_0_0"
           targetNamespace="http://services.sabre.com/hotel/content/v4_0_0"
           elementFormDefault="qualified">

    <xs:simpleType name="NumericLengthwith3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length exactly 5.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[1-5 ]{1}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OTACodeType">
        <xs:annotation>
            <xs:documentation>Positive Integer to denote OTA Codes</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength1to3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 1 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!--<xs:pattern value="[A-Z0-9]{3}"/>-->
            <xs:minLength value="1"/>
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AlphaNumericLength4">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length is 4.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]{4}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength3to10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength3to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength2to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 2 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength1to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength3to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for boolean Strings, length 0 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength3WithNoSpace">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 1 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]{3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length  2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to4">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 4.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,4}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to5">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 5.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,5}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UpperCaseAlphanumericStringLength1to8">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Upper Case Character Strings, length 1 to 8.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{1,8}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to255">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 255.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="255"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to500">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 500.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="500"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NumericLength10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Numeric, length from 1 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9 ]{1,10}"/>
        </xs:restriction>
    </xs:simpleType>



    <xs:simpleType name="ImageTypes">
        <xs:annotation>
            <xs:documentation>
                Image types
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="ORIGINAL"/>
            <xs:enumeration value="THUMBNAIL"/>
            <xs:enumeration value="SMALL"/>
            <xs:enumeration value="MEDIUM"/>
            <xs:enumeration value="LARGE"/>
        </xs:restriction>
    </xs:simpleType>



    <!-- Hotel Content V2.0.0 Request Elements -->

    <xs:complexType name="HotelRef">
        <xs:annotation>
            <xs:documentation>Describes the reference to a Hotel by its code. This includes an attribute for Hotel Code</xs:documentation>
        </xs:annotation>
        <xs:attribute name="HotelCode" type="StringLength1to20" use="required">
            <xs:annotation>
                <xs:documentation>Refers to hotel code by which a hotel is identified</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CodeContext" type="CodeContextType" use="optional" default="SABRE">
            <xs:annotation>
                <xs:documentation xml:lang="en">Indicates code context type.Default code context is SABRE.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="CodeContextType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Indicates code context type.Default code context is SABRE.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="GLOBAL">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Code context type is GLOBAL</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SABRE">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Code context type is SABRE</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="DescriptiveInfoRef">
        <xs:annotation>
            <xs:documentation>Descriptions about the property and facilities</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="PropertyInfo" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                    <xs:documentation>A boolean flag indicating if Property information is expected in the response</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LocationInfo" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                    <xs:documentation>A boolean flag indicating if Location information is expected in the response</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amenities" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                    <xs:documentation>A boolean flag indicating if Amenities information is expected in the response</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Descriptions" type="DescriptionsRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Includes the supported types for which the Descriptions are expected in the response</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecurityFeatures" type="xs:boolean" minOccurs="0"  default="false"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DescriptionsRef">
        <xs:annotation>
            <xs:documentation>Includes the supported types for which the Descriptions are expected in the response</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="13">
            <xs:element name="Description">
                <xs:annotation>
                    <xs:documentation>Indicates the supported types for which the Descriptions are expected</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="DescriptionType" use="required">
                        <xs:annotation>
                            <xs:documentation>Indicates the supported types for which the Descriptions are expected</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:simpleType name="DescriptionType">
        <xs:annotation>
            <xs:documentation>
                Description Types
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ShortDescription"/>
            <xs:enumeration value="Dining"/>
            <xs:enumeration value="Facilities"/>
            <xs:enumeration value="Recreation"/>
            <xs:enumeration value="Services"/>
            <xs:enumeration value="Attractions"/>
            <xs:enumeration value="CancellationPolicy"/>
            <xs:enumeration value="DepositPolicy"/>
            <xs:enumeration value="Directions"/>
            <xs:enumeration value="Policies"/>
            <xs:enumeration value="SafetyInfo"/>
            <xs:enumeration value="TransportationInfo"/>
            <xs:enumeration value="GuaranteePolicy"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="MediaRef">
        <xs:annotation>
            <xs:documentation xml:lang="en">Specify the media types and other parameters of MediaItems to be returned.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="MediaTypes" type="MediaTypes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Media Types</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Categories" type="Categories" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Image Category</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RoomTypeCodes" type="RoomTypeCodes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">List of Room Type Codes</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AdditionalInfo" type="AdditionalInfoRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Additional Information related to the image</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Languages" type="Languages" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Languages</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="MaxItems" type="MaxImagesType" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">Maximum images requested by the user. Valid values are "ALL" or 1 to 99
                    images.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="MediaTypes">
        <xs:annotation>
            <xs:documentation xml:lang="en">Media Types</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="Images" type="Images" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Images</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PanoramicMedias" type="PanoramicMedias" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Panoramic Medias</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Videos" type="Videos" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">List Of Videos</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Categories">
        <xs:annotation>
            <xs:documentation xml:lang="en">Image Category</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="22">
            <xs:element name="Category">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Image Category</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Code" type="xs:positiveInteger" use="required">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Image Category Code</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RoomTypeCodes">
        <xs:annotation>
            <xs:documentation xml:lang="en">List of Room Type Codes</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="22">
            <xs:element name="RoomTypeCode">
                <xs:annotation>
                    <xs:documentation xml:lang="en">RoomTypeCode</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Code" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Specific system room type code, ex: A1K, A1Q etc.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AdditionalInfoRef">
        <xs:annotation>
            <xs:documentation xml:lang="en">Image related additional information types</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="10">
            <xs:element name="Info">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Image  information</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:boolean">
                            <xs:attribute name="Type" type="AdditionalInfoType" use="required">
                                <xs:annotation>
                                    <xs:documentation>
                                        Image  additional information types
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Languages">
        <xs:annotation>
            <xs:documentation xml:lang="en">Specify the Languages</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="10">
            <xs:element name="Language">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Specify the Language</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Code" type="LanguageCode">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Two character Language Code</xs:documentation>
                        </xs:annotation>

                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="MaxImagesType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Maximum images requested by the user. Valid values are "ALL" or 1 to 99
                images.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(ALL|([1-9]{1})|([0-9]{2}))"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="Images">
        <xs:annotation>
            <xs:documentation xml:lang="en">Images</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="5">
            <xs:element name="Image">
                <xs:annotation>
                    <xs:documentation>image attributes description</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="ImageTypes" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                Image types
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PanoramicMedias">
        <xs:annotation>
            <xs:documentation xml:lang="en">Panoramic Medias</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="5">
            <xs:element name="PanoramicMedia">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Panoramic Media</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="PanoramaMediaTypes" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                Panorama Media types
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="Videos">
        <xs:annotation>
            <xs:documentation xml:lang="en">List Of Videos</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="5">
            <xs:element name="Video">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Video Info</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="VideoTypes" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                Video types
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="PanoramaMediaTypes">
        <xs:annotation>
            <xs:documentation>
                Panorama Media types
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="HD360"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="VideoTypes">
        <xs:annotation>
            <xs:documentation>
                Video types
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="VIDEO360"/>
            <xs:enumeration value="VIDEO720"/>
            <xs:enumeration value="VIDEOTHUMBNAIL"/>
            <xs:enumeration value="ALL"/>
        </xs:restriction>
    </xs:simpleType>


    <!-- Request elements End -->

    <xs:complexType name="HotelInfo">
        <xs:annotation>
            <xs:documentation>
                Hotel summary information
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="TierLabels" type="TierLabels" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Agency or Corporate labels assigned for this property</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="HotelCode" type="StringLength0to20" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">Property Code/ID</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CodeContext" type="CodeContextType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">Indicates whether the given HotelCode is Sabre property ID or Global property ID.
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="HotelName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Property Name
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="ChainCode" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Chain code
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="ChainName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Chain Name
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BrandCode" type="StringLength0to50" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Brand Code
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BrandName" type="StringLength0to500" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Brand Name
                </xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="Status" type="PropertyStatusType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Hotel Status Types
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SabreRating" type="StringLength0to10" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Sabre property rating
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SabreHotelCode" type="StringLength1to20">
            <xs:annotation>
                <xs:documentation xml:lang="en">Holds SabreID</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Logo" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>Logo</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="TierLabels">
        <xs:annotation>
            <xs:documentation>Agency or Corporate labels assigned for this property</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="5">
