<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://services.sabre.com/hotel/pricecheck/v4_0_0"
           targetNamespace="http://services.sabre.com/hotel/pricecheck/v4_0_0"
           elementFormDefault="qualified">

    <!-- Request elements -->
    <xs:complexType name="POS">
        <xs:annotation>
            <xs:documentation>Point of sales information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Source">
                <xs:annotation>
                    <xs:documentation>Element to Hold PCC value</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="PseudoCityCode" type="AlphaNumericLength1to4" use="required">
                        <xs:annotation>
                            <xs:documentation>"PseudoCityCode" (required) Four-character pseudo city code (PCC)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RateInfoRef">
        <xs:annotation>
            <xs:documentation>Hotel rates request message to get details of available rates hotels.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="RateKey" type="RateKeyType" use="required">
            <xs:annotation>
                <xs:documentation>Encrypted key that holds product details.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="RateKeyType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Restricting Empty Strings</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Response elements -->
    <xs:simpleType name="NumericStringLength1to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Numeric Strings, length 1 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{1,2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Amount">
        <xs:annotation>
            <xs:documentation xml:lang="en">Specifies an amount, max 2 decimals.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
            <xs:fractionDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PriceDifferenceAmount">
        <xs:annotation>
            <xs:documentation xml:lang="en">Specifies an amount, max 2 decimals.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength3WithNoSpace">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 1 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]{3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CodeContextType">
        <xs:annotation>
            <xs:documentation xml:lang="en">The code that signifies the GLOBAL as code context.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="GLOBAL">
                <xs:annotation>
                    <xs:documentation xml:lang="en">GLOBAL</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SABRE">
                <xs:annotation>
                    <xs:documentation xml:lang="en">SABRE information</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String length 0 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,3}" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to5">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 5.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,5}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for boolean Strings, length 0 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="10" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength1to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to255">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 255.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="255" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UnitOfMeasureType">
        <xs:annotation>
            <xs:documentation>
                Used for specifing what unit will be used for search radius distance.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="MI" />
            <xs:enumeration value="KM" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StringLength0to500">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 500.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="500" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UpperCaseAlphaNumericLength2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{2}" />
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="LocationInfo">
        <xs:annotation>
            <xs:documentation xml:lang="en">Location and Address details for the Hotel Property.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="Address" type="Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Address details for the Hotel Property.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Neighborhoods" type="Neighborhoods" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Neighborhoods</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Contact" type="Contact" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contact details for the Hotel Property.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Latitude" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation>Latitude of the  the Hotel Property</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Longitude" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation>Longitude of the  the Hotel Property</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="Address">
        <xs:annotation>
            <xs:documentation xml:lang="en">Address details for the Hotel Property.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="AddressLine1" type="StringLength0to255">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Address Line1</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressLine2" type="StringLength0to255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Address Line2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressLine3" type="StringLength0to255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Address Line3</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CityName">
                <xs:annotation>
                    <xs:documentation xml:lang="en">City Name</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="StringLength0to50">
                            <xs:attribute name="CityCode" type="StringLength0to5" use="optional">
                                <xs:annotation>
                                    <xs:documentation>Primary Airport code referred as city code</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="StateProv" type="StateProv" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">State or Province of the Hotel Property</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PostalCode" type="StringLength0to20" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">PostalCode of the Hotel Property</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountryName" type="CountryName">
                <xs:annotation>
                    <xs:documentation xml:lang="en">CountryName of the Hotel Property</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="StateProv">
        <xs:annotation>
            <xs:documentation xml:lang="en">State or Province of the Hotel Property</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="StateCode" type="StringLength0to10" use="optional">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">State of the Hotel Property</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="CountryName">
        <xs:annotation>
            <xs:documentation>
                Country Name of Hotel Property
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="Code" type="StringLength0to3" use="optional">
                    <xs:annotation>
                        <xs:documentation>
                            Country Code of Hotel Property
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="Neighborhoods">
        <xs:annotation>
            <xs:documentation>Neighborhoods have polygonId and localArea value</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0" maxOccurs="10">
            <xs:element name="Neighborhood">
                <xs:annotation>
                    <xs:documentation>Neighborhood should be list type</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="Id" type="StringLength0to20" use="optional">
                                <xs:annotation>
                                    <xs:documentation>
                                        polygon id of Neighborhood
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Contact">
        <xs:annotation>
            <xs:documentation>Contact information for a hotel</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Phone" type="StringLength0to50" use="optional">
            <xs:annotation>
                <xs:documentation>Hotel phone number length 0 to 20</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Fax" type="StringLength0to20" use="optional">
            <xs:annotation>
                <xs:documentation>Hotel fax number length 0 to 20</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="RateInfos">
        <xs:annotation>
            <xs:documentation>
                Collection of Rate information for a request from all suppliers.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RateInfo" type="RateInfo" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Provides details of a product in the supplier provided currency.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConvertedRateInfo" type="RateInfo" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Provides details of a product in the requested currency.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Rooms">
        <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element name="Room" type="RoomType">
                <xs:annotation>
                    <xs:documentation>Describes hotel rooms information for a hotel.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RateInfo">
        <xs:annotation>
            <xs:documentation>Rate information for the shop request. Total Rates (not room specific).</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Commission" type="CommissionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Commissions applicable.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="StartDate" type="Date">
            <xs:annotation>
                <xs:documentation>Start date.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="EndDate" type="Date">
            <xs:annotation>
                <xs:documentation>End date.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AmountBeforeTax" type="Amount">
            <xs:annotation>
                <xs:documentation>Specifies an amount before tax.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AmountAfterTax" type="Amount">
            <xs:annotation>
                <xs:documentation>Specifies an amount after tax.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MinSellingRate" type="Amount">
            <xs:annotation>
                <xs:documentation>specifies minimum selling rate</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AverageNightlyRate" type="Amount">
            <xs:annotation>
                <xs:documentation>Average rate per night.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AverageNightlyRateBeforeTax" type="Amount">
            <xs:annotation>
                <xs:documentation xml:lang="en">Average nightly rate</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace">
            <xs:annotation>
                <xs:documentation>Currency code in which the rate is represented.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AdditionalFeesInclusive" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Boolean attribute which indicates whether additional fees are included in the total
                    rate.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TaxInclusive" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Boolean attribute which indicates whether taxes are included in the total rate.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="LocalFeesInclusive" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Boolean attribute which indicates whether local fees are included in the total rate.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="IncidentalsInclusive" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Boolean attribute which indicates whether incidentals are included in the total
                    rate.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RateSource" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Numeric value representing aggregator that provided the rate.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RateKey" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Encrypted string that holds product details.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="RoomExtrasType">
        <xs:sequence>
            <xs:element name="RoomExtra" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:annotation>
                        <xs:documentation>List of room extras available with chosen rate plan.</xs:documentation>
                    </xs:annotation>
                    <xs:sequence minOccurs="0">
                        <xs:element name="Text" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Description of RoomExtras.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Code" type="OTACodeType">
                        <xs:annotation>
                            <xs:documentation>Code applicable for the extra rooms</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>OTA Code Description</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Amount" type="Amount">
                        <xs:annotation>
                            <xs:documentation>Amount for extra room</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace">
                        <xs:annotation>
                            <xs:documentation>Currency code applicable for the extra rooms</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="FeesType">
        <xs:sequence>
            <xs:element name="Fee" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="FeeDescription">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Text" minOccurs="0" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Code" type="OTACodeType">
                        <xs:annotation>
                            <xs:documentation>Refer to OpenTravel Code List Fee Tax Type (FTT)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Amount" type="Amount">
                        <xs:annotation>
                            <xs:documentation>Chargeable rate for room extra</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace" use="required">
                        <xs:annotation>
                            <xs:documentation>Currency code applicable for the rates.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Amount" type="Amount">
            <xs:annotation>
                <xs:documentation>Chargeable rate for room extra</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace" use="required">
            <xs:annotation>
                <xs:documentation>Currency code applicable for the rates.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="RateTaxesType">
        <xs:sequence>
            <xs:element name="Tax" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Represents a tax type applicable.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:annotation>
                        <xs:documentation>Represents a tax type applicable.</xs:documentation>
                    </xs:annotation>
                    <xs:attribute name="StartDate" type="Date">
                        <xs:annotation>
                            <xs:documentation>Start date.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="EndDate" type="Date">
                        <xs:annotation>
                            <xs:documentation>End date.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Amount" type="Amount" use="required">
                        <xs:annotation>
                            <xs:documentation>Chargeable rate for room extra</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace" use="required">
                        <xs:annotation>
                            <xs:documentation>Currency code applicable for the rates.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="TaxGroups" type="TaxGroupsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Represents a tax type applicable.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Amount" type="Amount">
            <xs:annotation>
                <xs:documentation>Tax Amount.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace">
            <xs:annotation>
                <xs:documentation>Currency code applicable for the rates.</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="TaxGroupsType">
        <xs:sequence>
            <xs:element name="TaxGroup" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Represents a tax type applicable.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:annotation>
                        <xs:documentation>Represents a tax type applicable.</xs:documentation>
                    </xs:annotation>
                    <xs:sequence>
                        <xs:element name="TaxDescription" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Description of tax.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:annotation>
                                    <xs:documentation>Description of tax.</xs:documentation>
                                </xs:annotation>
                                <xs:sequence>
                                    <xs:element name="Text" type="xs:string" maxOccurs="unbounded"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Code" type="OTACodeType" use="required">
                        <xs:annotation>
                            <xs:documentation>Refer to OpenTravel Code List Fee Tax Type (FTT).</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>OTA Code Description</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Amount" type="Amount" use="required">
                        <xs:annotation>
                            <xs:documentation>Chargeable rate for room extra</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace" use="required">
                        <xs:annotation>
                            <xs:documentation>Currency code applicable for the rates.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RateFeesType">
        <xs:annotation>
            <xs:documentation>Provides information of the different types of fees.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Fee" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Provides information of the different types of fees.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:annotation>
                        <xs:documentation>Provides information of the different types of fees.</xs:documentation>
                    </xs:annotation>
                    <xs:attribute name="StartDate" type="Date">
                        <xs:annotation>
                            <xs:documentation>Start date.</xs:documentation>
                        </xs:annotation>
