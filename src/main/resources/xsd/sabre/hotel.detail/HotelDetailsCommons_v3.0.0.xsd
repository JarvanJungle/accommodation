<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://services.sabre.com/hotel/details/v3_0_0"
           targetNamespace="http://services.sabre.com/hotel/details/v3_0_0"
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:simpleType name="NumericLengthwith3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length exactly 5.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[1-5 ]{1}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NumericLength10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Numeric, length from 1 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9 ]{1,10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to5">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 5.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,5}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength1To20WithNoSpace">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for numeric, length 1 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="preserve" />
            <xs:minLength value="1" />
            <xs:maxLength value="20" />
            <xs:pattern value="[0-9]+" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength3to10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3" />
            <xs:maxLength value="10" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength1to15">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 15.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="15" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to1000">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="1000" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NumericStringLength1to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Numeric Strings, length 1 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{1,2}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength3WithNoSpace">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 1 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]{3}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength1to3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 1 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="3" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength1to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AlphaLength1to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length 1 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z]{1,2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength3to20">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 20.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3" />
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength3to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 3 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3" />
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength2to50">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 2 to 50.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2" />
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AlphaLength3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length exactly 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z]{3}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to10">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for boolean Strings, length 0 to 10.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="10" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to3">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,3}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 2.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{0,2}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="UpperCaseAlphanumericStringLength1to8">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Upper Case Character Strings, length 1 to 8.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{1,8}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to255">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 255.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="255" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to500">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 500.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="500" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StringLength0to100">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 100.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0" />
            <xs:maxLength value="100" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="UpperCaseAlphaLength1to2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for an Alpha String, length 1 to 2 (for letter codes).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]{1,2}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AlphaLength2">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used for Character String, length 0 to 3.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z]{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="POS">
        <xs:annotation>
            <xs:documentation>Point of sales information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Source">
                <xs:annotation>
                    <xs:documentation>Element to Hold PCC value</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="PseudoCityCode" type="AlphaNumericLength4" use="required">
                        <xs:annotation>
                            <xs:documentation>"PseudoCityCode" (required) Four-character pseudo city code (PCC)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="HotelRefs">
        <xs:annotation>
            <xs:documentation xml:lang="en">Used to give hotel info</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="1" maxOccurs="1">
            <xs:element name="HotelRef">
                <xs:annotation>
                    <xs:documentation>HotelRefs is Mandatory.
                        Only one HotelRef can be given in a single request.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="HotelCode" type="StringLength1to20" use="required">
                        <xs:annotation>
                            <xs:documentation>Holds GlobalID or SabreID
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CodeContext" type="CodeContextType" use="optional" default="SABRE">
                        <xs:annotation>
                            <xs:documentation>GLOBAL/SABRE Default will be SABRE</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="CodeContextType">
        <xs:annotation>
            <xs:documentation xml:lang="en">The code that signifies the GLOBAL as code context.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="GLOBAL">
                <xs:annotation>
                    <xs:documentation xml:lang="en">GLOBAL properties</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SABRE">
                <xs:annotation>
                    <xs:documentation xml:lang="en">SABRE</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="RateInfoRef">
        <xs:annotation>
            <xs:documentation>Hotel Rates request Message to get details of available rates hotels.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="StayDateTimeRange" type="DateRangeTypeRef">
                <xs:annotation>
                    <xs:documentation>Specifies start date and end date of hotel stay duration</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RateRange" type="RateRange" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Indicates rate range which include min and max rate</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Rooms">
                <xs:annotation>
                    <xs:documentation>Collection of rooms</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RoomSetTypes" type="RoomSetTypes" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>List of Attributes on which rooms are grouped</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Room" type="Room" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Room Info</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="RatePlanCandidates" type="RatePlanCandidates" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Collection of RatePlanCandidates</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LoyaltyIds" type="LoyaltyIds" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>"LoyaltyIds" is used to specify a Loyality number if applicable. Max of 10 LoyaltyID's</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FrequentFlyerNumber" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Customer frequent flyer number.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CorpDiscount" type="CorpDiscountCode" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Corporate discount ID/Number.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RateSource" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Used to specify the source of the data being exchanged as determined by trading partners. Used to specify the source of the data being exchanged.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="CurrencyCode" type="StringLength3WithNoSpace" use="optional">
            <xs:annotation>
                <xs:documentation>Requesting rates to be returned in particular currency .</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PrepaidQualifier" type="PrepaidQualifierType" default="IncludePrepaid">
            <xs:annotation>
                <xs:documentation>'ExcludePrepaid' - Exclude prepaid rates from the response.
                    'IncludePrepaid' -	Return prepaid and non-prepaid rates.
                    'PrepaidOnly' -	Only return prepaid rates.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RefundableOnly" type="xs:boolean" default="false">
            <xs:annotation>
                <xs:documentation>Optional 'true' or 'false'. 'true' returns refundable rates only. 'false' returns both refundable and non-refundable rates.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ConvertedRateInfoOnly" type="xs:boolean" default="true">
            <xs:annotation>
                <xs:documentation>Optional 'true' or 'false'. 'true' returns  rates in only requested currency. 'false' returns rates in both requested and supplier currency.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TravellerCountry" type="AlphaLength2">
            <xs:annotation>
                <xs:documentation>
                    Traveller Country Code
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ShowNegotiatedRatesFirst" type="xs:boolean" default="false">
            <xs:annotation>
                <xs:documentation>
                    Flag to show negotiated rates first when true with rest of the rates sorted in ascending order of AmountBeforeTax. When false, all rates will be sorted in ascending order of AmountBeforeTax. Default value is false.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="RateKeyRef">
        <xs:sequence>
            <xs:element name="RateRange" type="RateRange" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Indicates rate range which include min and max rate
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RoomSetTypes" type="RoomSetTypes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>List of Attributes on which rooms are grouped</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RateSource" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Used to specify the source of the data being exchanged as determined by trading partners.Used to specify the source of the data being exchanged.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="RateKey" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    Requesting rates to be returned in particular currency .
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PrepaidQualifier" type="PrepaidQualifierType" default="IncludePrepaid">
            <xs:annotation>
                <xs:documentation>
                    'ExcludePrepaid' - Exclude prepaid rates from the response. 'IncludePrepaid' - Return prepaid and non-prepaid rates. 'PrepaidOnly' - Only return prepaid rates.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RefundableOnly" type="xs:boolean" default="false">
            <xs:annotation>
                <xs:documentation>
                    Optional 'true' or 'false'. 'true' returns refundable rates only. 'false' returns both refundable and non-refundable rates.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ConvertedRateInfoOnly" type="xs:boolean" default="true">
            <xs:annotation>
                <xs:documentation>
                    Optional 'true' or 'false'. 'true' returns rates in only requested currency. 'false' returns rates in both requested and supplier currency.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ExactMatchOnly" type="xs:boolean" default="false">
            <xs:annotation>
                <xs:documentation>
                    Flag to show negotiated rates first when true with rest of the rates sorted in ascending order of AmountBeforeTax. When false, all rates will be sorted in ascending order of AmountBeforeTax. Default value is false.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TravellerCountry" type="AlphaLength2">
            <xs:annotation>
                <xs:documentation>
                    Traveller Country Code
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ShowNegotiatedRatesFirst" type="xs:boolean" default="false">
            <xs:annotation>
                <xs:documentation>
                    Flag to show negotiated rates first when true with rest of the rates sorted in ascending order of AmountBeforeTax. When false, all rates will be sorted in ascending order of AmountBeforeTax. Default value is false.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ShopKey" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation> ShopKey to retrieve next set of rates</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="CorpDiscountCode">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9a-zA-Z]{1,20}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="DateRangeTypeRef">
        <xs:annotation>
            <xs:documentation>Indicates stay date range which includes start date and end date
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="StartDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">Start Date</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="EndDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">End Date</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CheckInTime" type="xs:string">
            <xs:annotation>
                <xs:documentation>Check in time</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CheckOutTime" type="xs:string">
            <xs:annotation>
                <xs:documentation>Check out time</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DurationInHours" type="xs:positiveInteger">
            <xs:annotation>
                <xs:documentation>Stay duration in hours</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="RoomSetTypes">
        <xs:annotation>
            <xs:documentation>List of Attributes on which rooms are grouped</xs:documentation>
        </xs:annotation>
        <xs:sequence maxOccurs="10">
            <xs:element name="RoomSet">
                <xs:annotation>
                    <xs:documentation>Each Attribute on which rooms are grouped</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="Type" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Type holds string value of attribute name</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RateRange">
        <xs:annotation>
            <xs:documentation>Indicates rate range which include min and max rate</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Min" type="Amount">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies Minimum amount, max 2 decimals.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Max" type="Amount">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies Maximum amount, max 2 decimals.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="Room">
        <xs:annotation>
            <xs:documentation>Room Info</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ChildAges" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Age of Children</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Index" type="xs:positiveInteger" use="required">
            <xs:annotation>
                <xs:documentation>Room Index</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Adults" type="xs:positiveInteger" use="required">
            <xs:annotation>
                <xs:documentation>Number of Adults</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Children" type="xs:nonNegativeInteger">
            <xs:annotation>
                <xs:documentation>Number of Children</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="BestOnly">
        <xs:annotation>
            <xs:documentation>
                Determine whether one lowest rate or multiple lowest rates are needed
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">LOWEST_RATE_ONLY</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation xml:lang="en">LOWEST_RATE_ALL_SOURCES</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="RatePlanCandidates">
        <xs:annotation>
            <xs:documentation>Allow max of 11 RatePlanCandidate tags for now, to match IRAC. This may change later</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RatePlanCandidate" maxOccurs="11">
                <xs:annotation>
                    <xs:documentation>Allow max of 11 RatePlanCandidate tags for now, to match IRAC. This may change later</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="RatePlanType" type="RatePlanTypes" >
                        <xs:annotation>
                            <xs:documentation>An enumerated type that allows the query to specify a rate category type, and provides major categories for comparison across brands. Refer to
                                OpenTravel Code List Rate Plan Type (RPT)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="RatePlanCode" type="xs:string" >
                        <xs:annotation>
